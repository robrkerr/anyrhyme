<!DOCTYPE html>
<html>
  <head>
    <title>GiftRAPPED API</title>
    <%= stylesheet_link_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <div class="flash">
      <% flash.each do |key, values| %>
        <% values.each do |value| %>
          <div class="flash" id="<%= key %>">
            <%= value %>
          </div>
        <% end %>
      <% end %>
    </div>

    <%= yield %>

  </body>
</html>
